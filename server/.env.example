# 3D射击游戏服务器环境配置
NODE_ENV=production
PORT=3001

# 允许的域名（生产环境需要修改）
ALLOWED_ORIGINS=https://your-domain.com,https://www.your-domain.com

# Socket.io配置
SOCKET_PING_TIMEOUT=60000
SOCKET_PING_INTERVAL=25000

# 安全配置
MAX_PLAYERS_PER_ROOM=8
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# 监控和日志
LOG_LEVEL=info
ENABLE_MONITORING=true